{
  "manifest_version": 2,
  "name": "ChatGPT Extension",
  "version": "1.0.0",
  "description": "A ChatGPT Chrome extension",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "scripts": [
      "service.js"
    ]
  }
}
